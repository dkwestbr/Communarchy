{namespace communarchy.templates.html.arg.point.pov.nodes}

/**
 *  Active view for vote/up form
 *
 *  @param upvoteAction Action to perform an vote/up
 */
{template .UpvoteActive}
<form method="post" action="{$upvoteAction}">
	<div class="pov-upvote-container">
		<input class="upvote-active" type="submit"></input>
	</div>
</form>
{/template}